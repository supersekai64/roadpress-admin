{
  "name": "nextjs-starter-template",
  "version": "1.0.0",
  "description": "Next.js 15 professional starter template with TypeScript, Tailwind CSS, shadcn/ui, GSAP, Lenis and advanced optimizations",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "keywords": [
    "nextjs",
    "nextjs15",
    "react",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "gsap",
    "lenis",
    "starter",
    "template",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nextjs-starter-template"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-clean.ps1",
    "optimize:images": "node scripts/optimize-images.js",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy.ps1",
    "build": "prisma generate --no-engine && next build",
    "postinstall": "prisma generate",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "start": "next start",
    "lint": "next lint",
    "kill": "powershell -Command \"Get-Process -Name node -ErrorAction SilentlyContinue | Stop-Process -Force\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "db:seed:sms": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed-sms-reports.ts",
    "db:create-admin": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/create-admin.ts",
    "db:setup": "pnpm docker:up && timeout /t 5 /nobreak && pnpm db:push && pnpm db:seed",
    "diagnose": "node scripts/diagnose-prisma.js",
    "setup:env": "node scripts/setup-env.js"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/supercluster": "^7.1.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "gsap": "^3.13.0",
    "lenis": "^1.3.11",
    "lucide-react": "^0.545.0",
    "mapbox-gl": "^2.15.0",
    "next": "^15.5.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "prisma": "^6.17.1",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-map-gl": "^7.1.7",
    "simple-icons": "^15.16.1",
    "supercluster": "^8.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@types/geojson": "^7946.0.16",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
