{
  "name": "nextjs-starter-template",
  "version": "1.0.0",
  "description": "Next.js 15 professional starter template with TypeScript, Tailwind CSS, shadcn/ui, GSAP, Lenis and advanced optimizations",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "keywords": [
    "nextjs",
    "nextjs15",
    "react",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "gsap",
    "lenis",
    "starter",
    "template",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nextjs-starter-template"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-clean.ps1",
    "optimize:images": "node scripts/optimize-images.js",
    "prebuild": "pnpm optimize:images",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "start": "next start",
    "lint": "next lint",
    "kill": "powershell -Command \"Get-Process -Name node -ErrorAction SilentlyContinue | Stop-Process -Force\""
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "lenis": "^1.3.11",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
