{
  "buildCommand": "pnpm build",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "functions": {
    "pages/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
