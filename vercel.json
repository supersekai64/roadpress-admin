{
  "buildCommand": "pnpm build",
  "framework": "nextjs",
  "installCommand": "pnpm install --frozen-lockfile",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NEXTAUTH_URL": "https://roadpress.superbien-works.fr"
  }
}
